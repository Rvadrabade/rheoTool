FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      phiE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    inletNorth
    {
        type            fixedValue;
        value           uniform 33;
    }
    inletSouth
    {
        type            fixedValue;
        value           uniform 33;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    inletWest
    {
        type            fixedValue;
        value           uniform 34.65;
    }
    frontAndBack
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
}

