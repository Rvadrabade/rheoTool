FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nA;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
	type            fixedValue;
	value          uniform 1;
    }
   
    walls
    {
	type            zeroGradient;       
    }

    outlet
    {
	type            zeroGradient;        
    }

    frontAndBack
    {
	type            empty;     
    }
}

